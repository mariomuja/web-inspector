{
  "buildCommand": "cd frontend && npm install && npm run build -- --configuration=production",
  "outputDirectory": "frontend/dist/frontend/browser",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

