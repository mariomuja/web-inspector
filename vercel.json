{
  "buildCommand": "npm run build -- --configuration=production",
  "outputDirectory": "dist/frontend/browser",
  "installCommand": "npm install",
  "framework": null,
  "cwd": "frontend",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}


